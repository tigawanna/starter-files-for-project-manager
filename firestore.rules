rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /payments/{payment=**} {
      allow read, write
    }
      match /users/{user=**} {
      allow read, write: if
          request.auth.uid==null;
          }
          
      match /shops/{shop=**} {
      allow read, write
          }
          
      match /paymenthistorys/{paymenthistory} {
      allow read, write: if
         request.auth.uid==null;
           }
      match /shoplist/{shoplist=**} {
      allow read, write
       
        }
      match /notifications/{notification=**} {
      allow read, write
          }


    
 }

}